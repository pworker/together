<%= simple_form_for(@event) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :start_at %>
  </div>

  <div class="form-actions">
    <%= f.button :submit , :class =>"btn btn-primary"%>
    <%= link_to t(".Back"), events_path, :class => "btn" %>
  </div>
<% end %>
